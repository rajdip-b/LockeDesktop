<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="530.0" prefWidth="245.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <Button fx:id="btnCollapseSidebar" layoutX="11.0" layoutY="14.0" mnemonicParsing="false" onAction="#onCollapseSidebarClicked" prefHeight="56.0" prefWidth="56.0" />
   <VBox layoutX="1.0" layoutY="170.0" prefHeight="256.0" prefWidth="245.0">
      <padding>
         <Insets left="5.0" right="5.0" />
      </padding>
      <HBox fx:id="boxAdd" onMouseEntered="#onHBoxHoveredIn" onMouseExited="#onHBoxHoveredOut" prefHeight="56.0" prefWidth="220.0" style="-fx-background-radius: 5;">
         <VBox.margin>
            <Insets left="5.0" right="5.0" top="12.0" />
         </VBox.margin>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
         <ImageView fx:id="imgAdd" fitHeight="34.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
               <Insets right="10.0" top="12.0" />
            </HBox.margin>
         </ImageView>
         <Label fx:id="lblAdd" prefHeight="56.0" prefWidth="177.0" text="Add">
            <font>
               <Font size="15.0" />
            </font>
         </Label>
      </HBox>
      <HBox fx:id="boxEdit" onMouseEntered="#onHBoxHoveredIn" onMouseExited="#onHBoxHoveredOut" prefHeight="56.0" prefWidth="220.0" style="-fx-background-radius: 5;">
         <VBox.margin>
            <Insets left="5.0" right="5.0" />
         </VBox.margin>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
         <ImageView fx:id="imgEdit" fitHeight="34.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
               <Insets right="10.0" top="12.0" />
            </HBox.margin>
         </ImageView>
         <Label fx:id="lblEdit" prefHeight="59.0" prefWidth="183.0" text="Edit">
            <font>
               <Font size="15.0" />
            </font>
         </Label>
      </HBox>
      <HBox fx:id="boxDelete" onMouseEntered="#onHBoxHoveredIn" onMouseExited="#onHBoxHoveredOut" prefHeight="56.0" prefWidth="220.0" style="-fx-background-radius: 5;">
         <VBox.margin>
            <Insets left="5.0" right="5.0" />
         </VBox.margin>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
         <ImageView fx:id="imgDelete" fitHeight="34.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
               <Insets right="10.0" top="12.0" />
            </HBox.margin>
         </ImageView>
         <Label fx:id="lblDelete" prefHeight="59.0" prefWidth="183.0" text="Delete">
            <font>
               <Font size="15.0" />
            </font>
         </Label>
      </HBox>
      <HBox fx:id="boxDeleteAll" onMouseEntered="#onHBoxHoveredIn" onMouseExited="#onHBoxHoveredOut" prefHeight="56.0" prefWidth="220.0" style="-fx-background-radius: 5;">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
         <ImageView fx:id="imgDeleteAll" fitHeight="34.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
               <Insets right="10.0" top="12.0" />
            </HBox.margin>
         </ImageView>
         <Label fx:id="lblDeleteAll" prefHeight="59.0" prefWidth="183.0" text="Wipe Off">
            <font>
               <Font size="15.0" />
            </font>
         </Label>
         <VBox.margin>
            <Insets left="5.0" right="5.0" />
         </VBox.margin>
      </HBox>
   </VBox>
   <HBox fx:id="boxGithub" layoutX="11.0" layoutY="460.0" onMouseEntered="#onHBoxHoveredIn" onMouseExited="#onHBoxHoveredOut" prefHeight="56.0" prefWidth="225.0" style="-fx-background-radius: 5;">
      <padding>
         <Insets left="10.0" right="10.0" />
      </padding>
      <ImageView fx:id="imgGitHub" fitHeight="34.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets right="10.0" top="12.0" />
         </HBox.margin>
      </ImageView>
      <Label fx:id="lblGithub" prefHeight="59.0" prefWidth="183.0" text="Github">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </HBox>
   <HBox layoutY="90.0" prefHeight="56.0" prefWidth="245.0" style="-fx-background-radius: 5;">
      <padding>
         <Insets left="10.0" right="10.0" />
      </padding>
      <TextField fx:id="txtSearch" prefHeight="36.0" prefWidth="222.0" promptText="Search by service...">
         <HBox.margin>
            <Insets top="12.0" />
         </HBox.margin>
      </TextField>
   </HBox>
</AnchorPane>
