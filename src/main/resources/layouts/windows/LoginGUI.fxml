<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="313.0" prefWidth="595.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.locker.controller.windows.LoginGUIController">
   <children>
      <Label layoutX="50.0" layoutY="140.0" prefHeight="51.0" prefWidth="226.0" text="What would you like to do?" textAlignment="CENTER">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <PasswordField fx:id="txtPassword" layoutX="50.0" layoutY="216.0" onKeyPressed="#onKeyPressed" prefHeight="36.0" prefWidth="226.0" promptText="Enter password" />
      <Button fx:id="btnOpen" layoutX="410.0" layoutY="148.0" mnemonicParsing="false" onAction="#onOpenClicked" prefHeight="36.0" prefWidth="112.0" text="Open" textFill="WHITE" />
      <Button fx:id="btnReset" layoutX="410.0" layoutY="216.0" mnemonicParsing="false" onAction="#onResetClicked" prefHeight="36.0" prefWidth="112.0" text="Reset" textFill="WHITE" />
      <Label fx:id="lblWelcome" layoutX="50.0" layoutY="29.0" prefHeight="51.0" prefWidth="470.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Line endX="488.0" layoutX="107.0" layoutY="103.0" startX="-100.0" />
   </children>
</AnchorPane>
