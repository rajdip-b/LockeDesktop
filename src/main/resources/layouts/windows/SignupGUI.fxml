<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="337.0" prefWidth="668.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.locker.controller.windows.SignupGUIController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="45.0" prefWidth="326.0" text="Create your personal locker">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <ImageView fx:id="imageView" fitHeight="265.0" fitWidth="326.0" layoutX="14.0" layoutY="59.0" pickOnBounds="true" />
      <TextField fx:id="txtUsername" layoutX="373.0" layoutY="116.0" onKeyPressed="#onKeyPressed" prefHeight="39.0" prefWidth="258.0" promptText="Username">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" layoutX="373.0" layoutY="182.0" onKeyPressed="#onKeyPressed" prefHeight="39.0" prefWidth="258.0" promptText="Password">
         <font>
            <Font size="14.0" />
         </font>
      </PasswordField>
      <Button fx:id="btnStart" layoutX="521.0" layoutY="291.0" mnemonicParsing="false" onAction="#onStartClicked" prefHeight="32.0" prefWidth="110.0" text="Start" textFill="WHITE" />
      <Line endX="567.0" layoutX="101.0" layoutY="66.0" startX="-100.0" />
   </children>
</AnchorPane>
