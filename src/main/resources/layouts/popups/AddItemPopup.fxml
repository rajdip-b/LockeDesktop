<AnchorPane onKeyPressed="#onEscapePressed" prefHeight="377.0" prefWidth="436.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.locker.controller.popups.AddItemPopupController">
   <children>
