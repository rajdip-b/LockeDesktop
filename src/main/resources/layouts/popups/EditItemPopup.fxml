<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane onKeyPressed="#onEscapePressed" prefHeight="383.0" prefWidth="530.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.locker.controller.popups.EditItemPopupController">
   <children>
      <Label layoutX="14.0" prefHeight="40.0" prefWidth="206.0" text="Edit Item">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <VBox fx:id="vBoxTxtFields" layoutX="181.0" layoutY="49.0" prefHeight="262.0" prefWidth="288.0" spacing="15.0">
         <children>
            <TextField fx:id="txtService" prefHeight="40.0" prefWidth="279.0" />
            <TextField fx:id="txtUsername" onKeyPressed="#onEnterClicked" prefHeight="40.0" prefWidth="279.0" />
            <TextField fx:id="txtPassword" onKeyPressed="#onEnterClicked" prefHeight="40.0" prefWidth="279.0" />
            <TextField fx:id="txtEmail" onKeyPressed="#onEnterClicked" prefHeight="40.0" prefWidth="279.0" />
            <TextField fx:id="txtCreated" prefHeight="40.0" prefWidth="279.0" />
         </children>
      </VBox>
      <VBox fx:id="vBoxLabels" layoutY="58.0" prefHeight="262.0" prefWidth="162.0" spacing="36.0">
         <children>
            <Label text="Service: " VBox.vgrow="NEVER">
               <VBox.margin>
                  <Insets left="90.0" />
               </VBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Label text="Username: ">
               <font>
                  <Font size="15.0" />
               </font>
               <VBox.margin>
                  <Insets left="70.0" />
               </VBox.margin>
            </Label>
            <Label text="Password: ">
               <font>
                  <Font size="15.0" />
               </font>
               <padding>
                  <Insets left="76.0" />
               </padding>
            </Label>
            <Label text="Email: ">
               <font>
                  <Font size="15.0" />
               </font>
               <VBox.margin>
                  <Insets left="104.0" />
               </VBox.margin>
            </Label>
            <Label text="Created: ">
               <font>
                  <Font size="15.0" />
               </font>
               <VBox.margin>
                  <Insets left="87.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
      <Button layoutX="348.0" layoutY="320.0" mnemonicParsing="false" onAction="#onApplyClicked" prefHeight="40.0" prefWidth="121.0" text="Apply" />
   </children>
</AnchorPane>
